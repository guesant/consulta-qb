{
  "compilerOptions": {
    "target": "es2016",
    "module": "esnext",
    "lib": [
      "EsNext",
      "dom"
    ],
    "moduleResolution": "NodeNext",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "strict": true,
    "skipLibCheck": true
  }
}
